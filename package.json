{
  "version": "0.0.0-development",
  "name": "oh-my-log",
  "description": "Beautiful console logs for your console applications with native string substitution",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "https://github.com/ahmadnassri/oh-my-log",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadnassri/oh-my-log.git"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "keywords": [
    "cli",
    "console",
    "info",
    "log"
  ],
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/oh-my-log/issues"
  },
  "scripts": {
    "lint": "snazzy && echint",
    "pretest": "npm run lint",
    "test": "FORCE_COLOR=true tap test",
    "coverage": "FORCE_COLOR=true tap test --reporter silent --coverage"
  },
  "config": {
    "oh-my-log": {
      "date": "isoTime",
      "prefix": "[%name:red] %date:blue:"
    }
  },
  "devDependencies": {
    "echint": "^2.1.0",
    "snazzy": "^6.0.0",
    "tap": "^10.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "dateformat": "^2.0.0",
    "debug-log": "^1.0.1",
    "extend": "^3.0.0",
    "furmat": "^1.0.0"
  }
}
